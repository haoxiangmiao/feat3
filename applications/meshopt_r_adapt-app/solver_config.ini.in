[ALGLIBMinLBFGS]
type = ALGLIBMinLBFGS
plot = 1
tol_abs = 1e-8
tol_rel = 1e-8
max_iter = 500
precon = none
lbfgs_dim = 10
keep_iterates = 0

[ALGLIBMinCG]
type = ALGLIBMinCG
plot = 1
tol_abs = 1e-8
tol_rel = 1e-8
max_iter = 500
direction_update = DaiYuan
keep_iterates = 0

[NLCG]
type = NLCG
precon = none
plot = 1
tol_abs = 1e-8
tol_rel = 1e-8
max_iter = 10000
linesearch = StrongWolfeLinesearch
direction_update = DaiYuan
keep_iterates = 0

[DuDvPrecon]
type = DuDvPrecon
dirichlet_boundaries = bottom top left right
slip_boundaries = inner
linear_solver = pcg

[StrongWolfeLinesearch]
type = StrongWolfeLinesearch
plot = 0
max_iter = 20
tol_decrease = 1e-3
tol_curvature = 0.3
keep_iterates = 0

[SecantLinesearch]
type = SecantLinesearch
plot = 0
initial_step = 1e-2
max_iter = 100
keep_iterates = 0

[PCG-MGV]
type = pcg
max_iter = 100
tol_rel = 1e-8
plot = 1
precon = mgv

[rich]
type = richardson
max_iter = 4
min_iter = 4
precon = jac

[jac]
type = jac
omega = 0.5

[mgv]
type = mgv
smoother = rich
coarse = pcg

[pcg]
type = pcg
max_iter = 10
tol_rel = 1e-8
precon = jac
