[ALGLIBMinLBFGS]
type = ALGLIBMinLBFGS
precon = none
tol_rel = 1e-8
max_iter = 10000
lbfgs_dim = 10
plot_mode = all
keep_iterates = 0

[ALGLIBMinCG]
type = ALGLIBMinCG
precon = none
tol_rel = 1e-8
max_iter = 10000
direction_update = DYHSHybrid
plot_mode = all
keep_iterates = 0

[NLCG]
type = NLCG
precon = DuDvPrecon
tol_rel = 1e-8
max_iter = 10000
linesearch = MQCLinesearch
direction_update = DYHSHybrid
plot_mode = all
keep_iterates = 0

[MQCLinesearch]
type = MQCLinesearch
plot_mode = none
max_iter = 20
tol_decrease = 1e-3
tol_curvature = 0.3
keep_iterates = 0

[secantlinesearch]
type = SecantLinesearch
plot_mode = none
initial_step = 1e-2
max_iter = 100
keep_iterates = 0

[DuDvPrecon]
type = DuDvPrecon
dirichlet_boundaries = bnd:b bnd:t bnd:l bnd:r bnd:c
fixed_reference_domain = 1
linear_solver = WEAK-PCG-MG

[PCG-JAC]
type = pcg
max_iter = 1000
tol_rel = 1e-8
precon = jac

[PCG-MG]
type = pcg
max_iter = 100
min_stag_iter = 1
tol_rel = 1e-8
plot_mode = all
precon = MG2

[WEAK-PCG-MG]
type = pcg
max_iter = 10
min_stag_iter = 1
tol_rel = 1e-2
tol_rel = 1e-3
plot_mode = summary
precon = MG2

[Richardson-MG]
type = richardson
plot_mode = iter
tol_rel = 1e-8
max_iter = 100
precon = MG2

[cg]
type = pcg
min_iter = 4
max_iter = 4
precon = none

[rich]
type = richardson
max_iter = 4
min_iter = 4
precon = jac

[jac]
type = jacobi
omega = 0.5

[MG1]
type = mg
hierarchy = s:rich-c:pcg
lvl_min = -1
lvl_max = 0
cycle = v

[MG2]
type = mg
hierarchy = s:cg-c:pcg
lvl_min = -1
lvl_max = 0
cycle = f

[s:rich-c:pcg]
type = hierarchy
smoother = rich
coarse = PCG-JAC

[s:cg-c:pcg]
type = hierarchy
smoother = cg
coarse = PCG-JAC
