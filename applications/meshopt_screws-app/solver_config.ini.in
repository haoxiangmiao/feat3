[ALGLIBMinLBFGS]
type = ALGLIBMinLBFGS
plot = iter
max_iter = 30000
tol_rel = 1e-8
lbfgs_dim = 10
keep_iterates = 0

[ALGLIBMinCG]
type = ALGLIBMinCG
plot = iter
tol_rel = 1e-8
max_iter = 30000
direction_update = DYHSHybrid
keep_iterates = 0

[NLCG]
type = NLCG
precon = none
plot = iter
tol_rel = 1e-8
max_iter = 30000
linesearch = mqclinesearch
direction_update = DYHSHybrid
keep_iterates = 0

[DuDvPrecon]
type = DuDvPrecon
dirichlet_boundaries = screw:i screw:o
linear_solver = PCG-Jacobi

[PCG-MGV]
type = pcg
max_iter = 500
tol_rel = 1e-8
plot = iter
precon = Richardson-MGV

[Richardson-MGV]
type = richardson
plot = iter
tol_rel = 1e-8
max_iter = 1
precon = mgv

[mqclinesearch]
type = MQCLinesearch
plot = none
max_iter = 20
tol_decrease = 1e-3
tol_curvature = 0.3
keep_iterates = 0

[rich]
type = richardson
max_iter = 4
min_iter = 4
precon = jac

[jac]
type = jacobi
omega = 0.5

[mgv]
type = mg
hierarchy = h1_a
lvl_min = 0
lvl_max = -1
cycle = v

[h1_a]
type = hierarchy
smoother = rich
coarse = PCG-Jacobi

[cg]
type = pcg
min_iter = 4
max_iter = 4

[PCG-Jacobi]
type = pcg
max_iter = 1000
tol_rel = 1e-8
precon = jac
