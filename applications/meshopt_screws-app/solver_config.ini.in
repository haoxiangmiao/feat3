[ALGLIBMinLBFGS]
type = ALGLIBMinLBFGS
plot = 1
max_iter = 1000
tol_rel = 1e-8
tol_abs = 1e-8
precon = none
lbfgs_dim = 10
keep_iterates = 0

[ALGLIBMinCG]
type = ALGLIBMinCG
precon = none
plot = 1
tol_rel = 1e-8
tol_abs = 1e-8
max_iter = 1000
direction_update = DYHSHybrid
keep_iterates = 0

[NLCG]
type = NLCG
precon = none
plot = 1
tol_rel = 1e-8
tol_abs = 1e-8
max_iter = 1000
linesearch = strongwolfelinesearch
direction_update = DYHSHybrid
keep_iterates = 0

[DuDvPrecon]
type = DuDvPrecon
dirichlet_boundaries = outer
linear_solver = PCG-MGV

[PCG-MGV]
type = pcg
max_iter = 250
tol_rel = 1e-8
plot = 1
precon = mgv

[Richardson-MGV]
type = richardson
plot = 1
tol_rel = 1e-8
max_iter = 1000
precon = mgv

[strongwolfelinesearch]
type = StrongWolfeLinesearch
plot = 0
max_iter = 20
tol_decrease = 1e-3
tol_curvature = 0.3
keep_iterates = 0

[secantlinesearch]
type = SecantLinesearch
plot = 0
initial_step = 1e-2
max_iter = 100
keep_iterates = 0

[rich]
type = richardson
max_iter = 3
min_iter = 3
precon = jac

[jac]
type = jac
omega = 0.7

[mgv]
type = mgv
smoother = rich
coarse = pcg

[pcg]
type = pcg
max_iter = 1000
tol_rel = 1e-8
precon = jac
