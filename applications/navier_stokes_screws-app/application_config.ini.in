[ApplicationSettings]
mesh_files = screws_2d_chart_bezier_24_28_smaller.xml screws_2d_mesh_quad_360_1_smaller.xml
#mesh_files = screws_2d_chart_bezier_24_28.xml screws_2d_mesh_quad_opt_ss_360_1.xml
#mesh_files = unit-square-small-quad.xml
#mesh_files = unit-square-small-tria.xml
#mesh_files = unit-circle-quad.xml
meshopt_config_file = ./meshopt_config.ini
solver_config_file = ./solver_config.ini
mesh_optimiser = HyperelasticityDefault
t_end = 5e-2
reynolds = 5e1
use_deformation = 0
solve_flow = 1
solve_mesh_optimisation = 1

[DomainControlSettings]
lvl_min = 0
lvl_max = 3
parti-type = parmetis fallback
parti-rank-elems = 4
adapt_mode = chart
z_min = 0.0
z_max = 1.0
slices = 4

[TimeDiscretisation]
delta_t = 1e-4
num_steps = 1
p_extrapolation_steps = 1
use_rotational_form = 1
ALE = impl
convection = impl
viscous = impl
